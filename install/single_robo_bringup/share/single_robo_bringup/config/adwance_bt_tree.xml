<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="Untitled">
    <RecoveryNode number_of_retries="1">
      <PipelineSequence>
        <RateController hz="1">
          <ComputePathToPose goal="{goal}"
                             planner_id="GridBased"
                             path="{path}"/>
        </RateController>
        <FollowPath controller_id="FollowPath"
                    path="{path}"/>
      </PipelineSequence>
      <Sequence>
        <BackUp backup_dist="0.5"
                backup_speed="0.15"/>
        <Wait wait_duration="5"/>
        <Spin spin_dist="1.57"/>
      </Sequence>
    </RecoveryNode>
  </BehaviorTree>
</root>
